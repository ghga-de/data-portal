{
  "name": "data-portal",
  "version": "2.2.9",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "lint": "ng lint",
    "lf": "ng lint --fix",
    "test": "jest --runInBand",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:parallel": "jest",
    "e2e": "PLAYWRIGHT_HTML_HOST=0.0.0.0 playwright test",
    "e2e:debug": "PLAYWRIGHT_HTML_HOST=0.0.0.0 playwright test --project chromium --debug",
    "e2e:headed": "PLAYWRIGHT_HTML_HOST=0.0.0.0 playwright test --project chromium --headed",
    "e2e:ui": "PLAYWRIGHT_HTML_HOST=0.0.0.0 playwright test --project chromium --ui",
    "e2e:report": "playwright show-report --host 0.0.0.0",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky",
    "docs": "compodoc -p tsconfig.doc.json -s -r 8081",
    "compodoc:build": "compodoc -p tsconfig.doc.json",
    "compodoc:build-and-serve": "compodoc -p tsconfig.doc.json -s -r 8081",
    "compodoc:serve": "compodoc -s",
    "toc": "markdown-toc -i README.md"
  },
  "packageManager": "pnpm@10.23.0",
  "type": "module",
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.3.13",
    "@angular/cdk": "^20.2.14",
    "@angular/common": "^20.3.13",
    "@angular/compiler": "^20.3.13",
    "@angular/core": "^20.3.13",
    "@angular/forms": "^20.3.13",
    "@angular/material": "^20.2.14",
    "@angular/material-date-fns-adapter": "^20.2.14",
    "@angular/platform-browser": "^20.3.13",
    "@angular/platform-browser-dynamic": "^20.3.13",
    "@angular/router": "^20.3.13",
    "@ngneat/cashew": "^5.3.0",
    "@tailwindcss/postcss": "^4.1.17",
    "angularx-qrcode": "^20.0.0",
    "date-fns": "^4.1.0",
    "js-yaml": "^4.1.1",
    "oidc-client-ts": "^3.4.1",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@angular-eslint/eslint-plugin": "^20.6.0",
    "@angular-eslint/eslint-plugin-template": "^20.6.0",
    "@angular-eslint/template-parser": "^20.6.0",
    "@angular/build": "^20.3.11",
    "@angular/cli": "^20.3.11",
    "@angular/compiler-cli": "^20.3.13",
    "@compodoc/compodoc": "^1.1.32",
    "@eslint/js": "^9.39.1",
    "@eslint/markdown": "^7.5.1",
    "@playwright/test": "^1.56.1",
    "@testing-library/angular": "^18.1.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "angular-eslint": "^20.6.0",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-boundaries": "^5.2.0",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-jsdoc": "^60.8.3",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-preset-angular": "^15.0.3",
    "msw": "^2.12.2",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4.1.17",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.47.0"
  },
  "msw": {
    "workerDirectory": [
      "src/mocks"
    ]
  }
}
