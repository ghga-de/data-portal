{
  "name": "data-portal",
  "version": "0.1.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "lint": "ng lint",
    "lf": "ng lint --fix",
    "test": "jest --runInBand",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:parallel": "jest",
    "e2e": "PLAYWRIGHT_HTML_HOST=0.0.0.0 playwright test",
    "e2e:debug": "PLAYWRIGHT_HTML_HOST=0.0.0.0 playwright test --project chromium --debug",
    "e2e:headed": "PLAYWRIGHT_HTML_HOST=0.0.0.0 playwright test --project chromium --headed",
    "e2e:report": "playwright show-report --host 0.0.0.0",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky",
    "docs": "compodoc -p tsconfig.doc.json -s -r 8081",
    "compodoc:build": "compodoc -p tsconfig.doc.json",
    "compodoc:build-and-serve": "compodoc -p tsconfig.doc.json -s -r 8081",
    "compodoc:serve": "compodoc -s",
    "toc": "markdown-toc -i README.md"
  },
  "packageManager": "pnpm@9.12.3",
  "type": "module",
  "private": true,
  "dependencies": {
    "@angular-eslint/eslint-plugin": "^19.0.2",
    "@angular-eslint/template-parser": "^19.0.2",
    "@angular/animations": "^19.1.2",
    "@angular/cdk": "^19.1.0",
    "@angular/common": "^19.1.2",
    "@angular/compiler": "^19.1.2",
    "@angular/core": "^19.1.2",
    "@angular/forms": "^19.1.2",
    "@angular/material": "^19.1.0",
    "@angular/platform-browser": "^19.1.2",
    "@angular/platform-browser-dynamic": "^19.1.2",
    "@angular/router": "^19.1.2",
    "@ngneat/cashew": "^4.1.0",
    "angularx-qrcode": "^19.0.0",
    "buffer": "^6.0.3",
    "js-yaml": "^4.1.0",
    "oidc-client-ts": "^3.1.0",
    "rxjs": "~7.8.1",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.1.3",
    "@angular/cli": "^19.1.3",
    "@angular/compiler-cli": "^19.1.2",
    "@compodoc/compodoc": "^1.1.26",
    "@eslint/markdown": "^6.2.1",
    "@playwright/test": "^1.49.1",
    "@testing-library/angular": "^17.3.5",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "angular-eslint": "^19.0.2",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-boundaries": "^5.0.1",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-jsdoc": "^50.6.2",
    "eslint-plugin-prettier": "^5.2.3",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.5.0",
    "msw": "^2.7.0",
    "postcss": "^8.5.1",
    "prettier": "3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.10",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.3",
    "typescript-eslint": "^8.21.0"
  },
  "msw": {
    "workerDirectory": [
      "src/mocks"
    ]
  }
}
