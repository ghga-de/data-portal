{
  "name": "data-portal",
  "version": "2.4.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "typecheck": "tsc -b --pretty false",
    "typecheck:watch": "tsc -b -w --pretty false",
    "typecheck:ng": "ng build --configuration development",
    "watch": "ng build --watch --configuration development",
    "lint": "ng lint",
    "lf": "ng lint --fix",
    "test": "ng test --watch=false",
    "test:watch": "ng test",
    "test:ui": "ng test --ui --runner-config vitest.ui.config.ts",
    "e2e": "PLAYWRIGHT_HTML_HOST=0.0.0.0 playwright test",
    "e2e:debug": "PLAYWRIGHT_HTML_HOST=0.0.0.0 playwright test --project chromium --debug",
    "e2e:headed": "PLAYWRIGHT_HTML_HOST=0.0.0.0 playwright test --project chromium --headed",
    "e2e:ui": "PLAYWRIGHT_HTML_HOST=0.0.0.0 playwright test --project chromium --ui",
    "e2e:report": "playwright show-report --host 0.0.0.0",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky",
    "docs": "compodoc -p tsconfig.doc.json -s -r 8081",
    "compodoc:build": "compodoc -p tsconfig.doc.json",
    "compodoc:build-and-serve": "compodoc -p tsconfig.doc.json -s -r 8081",
    "compodoc:serve": "compodoc -s",
    "toc": "markdown-toc-gen update README.md"
  },
  "packageManager": "pnpm@10.29.2",
  "type": "module",
  "private": true,
  "dependencies": {
    "@angular/animations": "^21.1.3",
    "@angular/cdk": "^21.1.3",
    "@angular/common": "^21.1.3",
    "@angular/compiler": "^21.1.3",
    "@angular/core": "^21.1.3",
    "@angular/forms": "^21.1.3",
    "@angular/material": "^21.1.3",
    "@angular/material-date-fns-adapter": "^21.1.3",
    "@angular/platform-browser": "^21.1.3",
    "@angular/platform-browser-dynamic": "^21.1.3",
    "@angular/router": "^21.1.3",
    "@ngneat/cashew": "^5.3.0",
    "@tailwindcss/postcss": "^4.1.18",
    "angularx-qrcode": "^21.0.4",
    "date-fns": "^4.1.0",
    "js-yaml": "^4.1.1",
    "ngx-mat-input-tel": "^21.0.0",
    "oidc-client-ts": "^3.4.1",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@angular-eslint/eslint-plugin": "^21.2.0",
    "@angular-eslint/eslint-plugin-template": "^21.2.0",
    "@angular-eslint/template-parser": "^21.2.0",
    "@angular/build": "^21.1.3",
    "@angular/cli": "^21.1.3",
    "@angular/compiler-cli": "^21.1.3",
    "@compodoc/compodoc": "^1.2.1",
    "@eslint/js": "^9.39.2",
    "@eslint/markdown": "^7.5.1",
    "@playwright/test": "^1.58.2",
    "@testing-library/angular": "^19.0.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^25.2.2",
    "@vitest/browser-playwright": "^4.0.18",
    "@vitest/ui": "4.0.18",
    "angular-eslint": "^21.2.0",
    "autoprefixer": "^10.4.24",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-boundaries": "^5.4.0",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-jsdoc": "^62.5.4",
    "eslint-plugin-prettier": "^5.5.5",
    "husky": "^9.1.7",
    "jsdom": "^28.0.0",
    "msw": "^2.12.9",
    "postcss": "^8.5.6",
    "prettier": "^3.8.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tailwindcss": "^4.1.18",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.54.0",
    "vitest": "^4.0.18"
  },
  "msw": {
    "workerDirectory": [
      "src/mocks"
    ]
  }
}
